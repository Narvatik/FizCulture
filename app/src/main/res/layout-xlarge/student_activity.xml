<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/studentContainer"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView38"
        android:layout_width="102dp"
        android:layout_height="53dp"
        android:layout_marginStart="268dp"
        android:layout_marginLeft="268dp"
        android:layout_marginTop="180dp"
        android:text="Норматив по наклонам: \n 13 см"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/openCalendar" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="90dp"
        android:layout_height="66dp"
        android:layout_marginStart="268dp"
        android:layout_marginLeft="268dp"
        android:layout_marginTop="112dp"
        android:text="Норматив по прыжкам: \n 230"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/openCalendar" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="84dp"
        android:layout_height="54dp"
        android:layout_marginStart="268dp"
        android:layout_marginLeft="268dp"
        android:layout_marginTop="32dp"
        android:text="Норматив по бегу: \n 13 сек"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/openCalendar" />

    <Button
        android:id="@+id/menuBtnStudent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:onClick="onClick"
        android:text="Меню"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/tests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="304dp"
        android:layout_marginLeft="304dp"
        android:layout_marginTop="4dp"
        android:onClick="onClick"
        android:text="Тесты"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/openCalendar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="140dp"
        android:onClick="onClick"
        android:text="Выберите неделю"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lesson1_date"
        android:layout_width="84dp"
        android:layout_height="45dp"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="168dp"
        android:text="Занятие от 10 апреля"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menuBtnStudent" />

    <TextView
        android:id="@+id/lesson2_date"
        android:layout_width="84dp"
        android:layout_height="45dp"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="112dp"
        android:text="Занятие от 13 апреля"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/openCalendar" />

    <TextView
        android:id="@+id/lesson3_date"
        android:layout_width="84dp"
        android:layout_height="45dp"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="180dp"
        android:text="Занятие от 16 апреля"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/openCalendar" />

    <Button
        android:id="@+id/open_standard_results"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="43dp"
        android:layout_marginRight="43dp"
        android:onClick="onClick"
        android:text="Нормативы"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/openCalendar"
        app:layout_constraintTop_toBottomOf="@+id/tests" />

    <CheckBox
        android:id="@+id/checkBox29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="32dp"
        android:checked="true"
        android:clickable="false"
        android:text="Посетил"
        app:layout_constraintStart_toEndOf="@+id/lesson1_date"
        app:layout_constraintTop_toBottomOf="@+id/openCalendar" />

    <CheckBox
        android:id="@+id/checkBox30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="112dp"
        android:clickable="false"
        android:text="Не посетил"
        app:layout_constraintStart_toEndOf="@+id/lesson1_date"
        app:layout_constraintTop_toBottomOf="@+id/openCalendar" />

    <CheckBox
        android:id="@+id/checkBox31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="180dp"
        android:clickable="false"
        android:text="Не посетил"
        app:layout_constraintStart_toEndOf="@+id/lesson1_date"
        app:layout_constraintTop_toBottomOf="@+id/openCalendar" />

    <TextView
        android:id="@+id/Student_information"
        android:layout_width="135dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="17dp"
        android:text="Информация о студенте"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menuBtnStudent" />

</android.support.constraint.ConstraintLayout>